{
  "name": "create-tailwind-app",
  "version": "1.0.1",
  "private": true,
  "description": "Build the HTML template with tailwind css.",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postcss:build": "postcss src/css/main.css -o src/assets/css/build/tailwind.css --minify",
    "postcss:watch": "postcss src/css/main.css -o src/assets/css/build/tailwind.css --watch"
  },
  "keywords": [
    "HTML template",
    "tailwindcss",
    "tailwind",
    "HTML",
    "css framework"
  ],
  "author": "Rajdeep Singh",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.0.24"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
